{
  "python.formatting.provider": "autopep8",
  "prettier.singleQuote": true,
  "prettier.jsxBracketSameLine": false,
  "prettier.trailingComma": "all",
  "editor.rulers": [80],
  "window.title": "${activeEditorLong}",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ce7b1088",
    "editorOverviewRuler.findMatchForeground": "#ff0000",
    "editorIndentGuide.activeBackground": "#AA3f00",
    "editorIndentGuide.background": "#292929",
    "editorBracketMatch.background": "#AA3f33",
    "editorBracketMatch.border": "#AA3f33",
    "tab.inactiveBackground": "#141414",
    "tab.inactiveForeground": "#c9c9c9",
    "tab.activeBorder": "#ff711f",
    "tab.unfocusedActiveBorder": "#000000",
    "titleBar.activeBackground": "#028dffd5",
    "editor.selectionHighlightBackground": "#74031f",
    "editor.selectionBackground": "#74031f"
  },
  "window.openFilesInNewWindow": "on",
  "terminal.integrated.fontSize": 20,
  "editor.formatOnSave": false,
  "editor.cursorStyle": "block",
  "editor.renderLineHighlight": "line",
  "editor.fontSize": 18,
  "editor.wordWrap": "on",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "window.zoomLevel": 0,
  "editor.minimap.enabled": false,
  "workbench.editor.openSideBySideDirection": "down",
  "workbench.editor.swipeToNavigate": true,
  "workbench.command"
  "breadcrumbs.enabled": true,
  // "flow.pathToFlow": "${workspaceRoot}/node_modules/.bin/flow",
  "eslint.nodePath": "${workspaceRoot}/node_modules/.bin/eslint",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
  ],
  "window.titleBarStyle": "custom",
  "gitlens.currentLine.enabled": false,
  "gitlens.blame.highlight.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
}
